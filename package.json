{
  "name": "bremertonsports",
  "version": "1.0.0",
  "description": "Website code of bremertonsports.com",
  "main": "index.js",
  "scripts": {
    "start": "python3 -m http.server 3000",
    "build": "npm run build:common && npm run build:fonts && npm run build:index && npm run build:soccer",
    "build:fonts": "npx tailwindcss -i ./src/inter.css -o ./dist/inter.css",
    "build:common": "npx tailwindcss -i ./src/common.css -o ./dist/common.css",
    "build:index": "npx tailwindcss -i ./src/index.css -o ./dist/index.css",
    "watch:index": "npx tailwindcss -i ./src/index.css -o ./dist/index.css --watch",
    "build:soccer": "npx tailwindcss -i ./src/soccer.css -o ./dist/soccer.css",
    "watch:soccer": "npx tailwindcss -i ./src/soccer.css -o ./dist/soccer.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decoy31/bremertonsports.git"
  },
  "author": "Tim Duffey <decoy31@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/decoy31/bremertonsports/issues"
  },
  "homepage": "https://github.com/decoy31/bremertonsports#readme",
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.4.0",
    "autoprefixer": "10.4.4",
    "tailwindcss": "3.0.23"
  }
}
