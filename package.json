{
  "name": "bremertonsports",
  "version": "1.0.0",
  "description": "Website code of bremertonsports.com",
  "main": "index.js",
  "scripts": {
    "start": "python3 -m http.server 3000",
    "build": "npm run build:fonts && npm run build:common && npm run build:index && npm run build:soccer && npm run build:rentals && npm run build:cornhole && npm run build:rules && npm run build:redefine-fitness && npm run build:ape && npm run build:rising-force-academy && npm run build:force-development-program && npm run build:silent-auction && npm run build:silent-auction-items",
    "build:fonts": "npx tailwindcss -i ./src/fonts.css -o ./dist/fonts.css",
    "build:common": "npx tailwindcss -i ./src/common.css -o ./dist/common.css",
    "build:index": "npx tailwindcss -i ./src/index.css -o ./dist/index.css",
    "watch:index": "npx tailwindcss -i ./src/index.css -o ./dist/index.css --watch",
    "build:soccer": "npx tailwindcss -i ./src/soccer.css -o ./dist/soccer.css",
    "watch:soccer": "npx tailwindcss -i ./src/soccer.css -o ./dist/soccer.css --watch",
    "build:rentals": "npx tailwindcss -i ./src/rentals.css -o ./dist/rentals.css",
    "watch:rentals": "npx tailwindcss -i ./src/rentals.css -o ./dist/rentals.css --watch",
    "build:cornhole": "npx tailwindcss -i ./src/cornhole.css -o ./dist/cornhole.css",
    "watch:cornhole": "npx tailwindcss -i ./src/cornhole.css -o ./dist/cornhole.css --watch",
    "build:rules": "npx tailwindcss -i ./src/rules.css -o ./dist/rules.css",
    "watch:rules": "npx tailwindcss -i ./src/rules.css -o ./dist/rules.css --watch",
    "build:redefine-fitness": "npx tailwindcss -i ./src/redefine-fitness.css -o ./dist/redefine-fitness.css",
    "watch:redefine-fitness": "npx tailwindcss -i ./src/redefine-fitness.css -o ./dist/redefine-fitness.css --watch",
    "build:ape": "npx tailwindcss -i ./src/ape.css -o ./dist/ape.css",
    "watch:ape": "npx tailwindcss -i ./src/ape.css -o ./dist/ape.css --watch",
    "build:rising-force-academy": "npx tailwindcss -i ./src/rising-force-academy.css -o ./dist/rising-force-academy.css",
    "watch:rising-force-academy": "npx tailwindcss -i ./src/rising-force-academy.css -o ./dist/rising-force-academy.css --watch",
    "build:silent-auction": "npx tailwindcss -i ./src/silent-auction/index.css -o ./dist/silent-auction/index.css",
    "watch:silent-auction": "npx tailwindcss -i ./src/silent-auction/index.css -o ./dist/silent-auction/index.css --watch",
    "build:silent-auction-items": "npx tailwindcss -i ./src/silent-auction/items.css -o ./dist/silent-auction/items.css",
    "watch:silent-auction-items": "npx tailwindcss -i ./src/silent-auction/items.css -o ./dist/silent-auction/items.css --watch"
  },
  "repository": {
    "type": "git",
